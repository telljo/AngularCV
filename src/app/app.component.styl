#wrapper 
    height 100%

header 
    width 100%
    position fixed
    z-index 10
    background-color #2c3e50
    height 60px
nav
    width: 500px
    margin auto
    display flex
    justify-content space-evenly
    align-items center
    height 60px
.navLink
   text-decoration none
   height 100%
   width 120px
   display flex
   justify-content center
   align-items center
   color #ffffff
   text-transform uppercase;
   letter-spacing 1px;
   font: 13px/1.4 Helvetica, arial, freesans, clean, sans-serif, "Segoe UI Emoji", "Segoe UI Symbol";
   font-weight 700

.active-link
    background-color:  #1a242f

.navLink:hover
    color #007bb5

p a, p a:visited  
    line-height inherit

